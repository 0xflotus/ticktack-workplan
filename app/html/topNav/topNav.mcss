TopNav {
  background-color: #fff
  height: 2.5rem
  padding: 1rem
  padding-bottom: .5rem

  display: flex
  justify-content: space-between

  div.left {
    display: flex
    
    div {
      font-size: 1rem 
      $colorFontSubtle
      cursor: pointer

      padding-bottom: .6rem
      margin: .5rem 1rem .5rem .5rem
      
      -active {
        $colorFontBasic
        border-bottom: 2px solid #222
      }
    }
  }

  div.right {
  }
}

